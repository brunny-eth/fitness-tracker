{
  "name": "fitness-tracker",
  "version": "1.0.0",
  "description": "A personal fitness tracking application",
  "type": "module",
  "scripts": {
    "start": "concurrently \"cd server && npm start\" \"cd client && npm start\"",
    "install-all": "npm install && cd client && npm install && cd ../server && npm install",
    "backup": "node backup-db.js"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.36.3",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.7",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.13.1",
    "react-router-dom": "^7.1.3"
  }
}